    <configParams>
        <configParam number="1" type="rangemapped" size="1" default="00">
            <name>
                <lang xml:lang="de">Sirenen- / Stroboskop Modus</lang>
                <lang xml:lang="en">Siren/Strobe Mode</lang>
              </name>
                  <description>
                <lang xml:lang="de">Der Anwender kann den Wert von 0 bis 2 ändern, um entweder den Sirenenton, das Blinklicht oder beides für den Alarmfall einzustellen.</lang>
                <lang xml:lang="en">User can change value from 0 to 2 to turn either Siren sound, Strobe light, or both when triggered.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">Alles aktiviert</lang>
                    <lang xml:lang="en">All Enable</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">nur Sirene</lang>
                    <lang xml:lang="en">Siren only</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="de">nur Blinklicht</lang>
                    <lang xml:lang="en">Strobe only</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="2" type="rangemapped" size="1" default="00">
            <name>
                <lang xml:lang="de">Automatische Alarmabschaltung</lang>
                <lang xml:lang="en">Alarm Auto Stop Time</lang>
              </name>
                  <description>
                <lang xml:lang="de">Der Anwender kann die Dauer des Sirenenalarms verändern.</lang>
                <lang xml:lang="en">User can change the duration of siren time when triggered.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de"></lang>
                    <lang xml:lang="en">30 seconds</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de"></lang>
                    <lang xml:lang="en">60 seconds</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="de"></lang>
                    <lang xml:lang="en">120 seconds</lang>
                  </description>
              </value>
            <value from="03" to="03">
                <description>
                    <lang xml:lang="de"></lang>
                    <lang xml:lang="en">Will Not Automatically Stop</lang>
                  </description>
              </value>
                </configParam>
      </configParams>
  
    
  
