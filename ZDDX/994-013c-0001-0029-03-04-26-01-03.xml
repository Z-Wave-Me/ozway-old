  <deviceDescription>
                <description>
            <lang xml:lang="en">Plugin Switch with meter functionality PAN16-RU</lang>
            <lang xml:lang="ru">Розеточный модуль с измерением энергопотребления PAN16-RU</lang>
          </description>
            <wakeupNote>
            <lang xml:lang="en"></lang>
            <lang xml:lang="ru"></lang>
          </wakeupNote>
            <inclusionNote>
            <lang xml:lang="en">Press On/Off button three times within 1.5 seconds</lang>
            <lang xml:lang="ru">Трижды нажмите кнопку Вкл/Выкл</lang>
          </inclusionNote>
            <productName>Smart Energy Plug In Switch PAN16-RU</productName>
            <brandName>Philio</brandName>
            <productLine>Philio Z-wave Products</productLine>
            <productCode>PAN16-RU</productCode>
            <productVersion>1.3</productVersion>
                    </deviceDescription>

    <commandClasses>
        <commandClass id="0020" inNIF="false" secure="true"/>
        <commandClass id="0025" inNIF="false" secure="true"/>
        <commandClass id="0027" inNIF="false" secure="true"/>
