    <configParams>
        <configParam number="1" type="rangemapped" size="1" default="01">
            <name>
                <lang xml:lang="de">Start Level</lang>
                <lang xml:lang="en">Start Level Bit</lang>
              </name>
                  <description>
                <lang xml:lang="de">definiert, ob die Angabe über den Startpunkt beim Dimmen in der Funkanweisung ignoriert werden soll</lang>
                <lang xml:lang="en">defines if the start level bit shall be ignores when transmitting a dim command to a dimmer</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">Ignorieren</lang>
                    <lang xml:lang="en">ignore</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">Nicht ignorieren</lang>
                    <lang xml:lang="en">dont ignore</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="3" type="rangemapped" size="1" default="00">
            <name>
                <lang xml:lang="de">Nachtlicht</lang>
                <lang xml:lang="en">Night Light</lang>
              </name>
                  <description>
                <lang xml:lang="de">aktiviert die Nachtlichtfunktion</lang>
                <lang xml:lang="en">Activates Night Light</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">LED blinkt bei Aktivität</lang>
                    <lang xml:lang="en">LED blinks on activity</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">Nachtlicht aktiv</lang>
                    <lang xml:lang="en">Night Light active</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="4" type="rangemapped" size="1" default="00">
            <name>
                <lang xml:lang="de">Schalter invertieren </lang>
                <lang xml:lang="en">Invert Switch</lang>
              </name>
                  <description>
                <lang xml:lang="de">invertiert die Belegung der Schaltwippe</lang>
                <lang xml:lang="en">Inverts Switching (on is upper part)</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">Nein</lang>
                    <lang xml:lang="en">dont invert</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">Ja</lang>
                    <lang xml:lang="en">invert</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="19" type="rangemapped" size="1" default="00">
            <name>
                <lang xml:lang="de">LED Indikator</lang>
                <lang xml:lang="en">LED Transmission Indication</lang>
              </name>
                  <description>
                <lang xml:lang="de">definiert wie die LED eine Indikator für die Funkübertragung ist</lang>
                <lang xml:lang="en">defines the behavior of the LED when transmitting data</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">kein Blinken</lang>
                    <lang xml:lang="en">no flicker</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">Binkt immer solange Funk aktiv ist</lang>
                    <lang xml:lang="en">flicker all time of data transmission</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="de">Blinkt nur für eine Sekunde</lang>
                    <lang xml:lang="en">flicker for one second only</lang>
                  </description>
              </value>
