<?xml version="1.0" encoding="utf-8"?>
<ZWaveDevice xmlns="http://www.pepper1.net/zwavedb/xml-schemata/z-wave" schemaVersion="2">

  <descriptorVersion>1</descriptorVersion>

  <deviceData>
            <manufacturerId value="010e"/>
        <productType value="0008"/>
    <productId value="0001"/>
    <libType value="03"/>
    <protoVersion value="03"/>
    <protoSubVersion value="63"/>
    <appVersion value="01"/>
    <appSubVersion value="01"/>
        <basicClass value="04"/>
            <genericClass value="40"/>
            <specificClass value="03"/>
        <optional value="true"/>
    <listening value="false"/>
    <routing value="false"/>
        <beamSensor>1000</beamSensor>
            <rfFrequency>US</rfFrequency>
                
      </deviceData>

  <deviceDescription>
                <description>
            <lang xml:lang="en">Z-Wave Controlled deadbolt door lock with keypad</lang>
          </description>
            <wakeupNote>
            <lang xml:lang="en"></lang>
          </wakeupNote>
            <inclusionNote>
            <lang xml:lang="en">1. Set the controller in inclusion mode
2. Touch the User Button until you hear two beeps.</lang>
          </inclusionNote>
            <productName>Danalock100</productName>
            <brandName>Danalock.com</brandName>
            <productLine>Danalock</productLine>
            <productCode>DanalockBTZU100Square</productCode>
                <batteryType>ER34615M</batteryType>
            <batteryCount>1</batteryCount>
            </deviceDescription>

    <commandClasses>
        <commandClass id="005e"/>
        <commandClass id="0022"/>
        <commandClass id="0080"/>
        <commandClass id="005a"/>
        <commandClass id="0072"/>
        <commandClass id="0073"/>
        <commandClass id="0098"/>
        <commandClass id="0062" version="2" secure="true" nonSecure="false"/>
        <commandClass id="0070" secure="true" nonSecure="false"/>
        <commandClass id="0071" version="3" secure="true" nonSecure="false"/>
        <commandClass id="008a" version="2" secure="true" nonSecure="false"/>
        <commandClass id="008b" secure="true" nonSecure="false"/>
        <commandClass id="004e" secure="true" nonSecure="false"/>
        <commandClass id="0063" secure="true" nonSecure="false"/>
        <commandClass id="0053" secure="true" nonSecure="false"/>
        <commandClass id="0085" version="2" secure="true" nonSecure="false"/>
        <commandClass id="0059" secure="true" nonSecure="false"/>
        <commandClass id="007a" version="2" secure="true" nonSecure="false"/>
        <commandClass id="0086" version="2" secure="true" nonSecure="false"/>
      </commandClasses>
  
    <assocGroups>
        <assocGroup number="1" maxNodes="1">
            <description>
                <lang xml:lang="en">Lifeline: all unsolicited reports are sent to the node in Lifeline.</lang>
              </description>
          </assocGroup>
      </assocGroups>
  
    <configParams>
        <configParam number="1" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Direction</lang>
              </name>
                  <description>
                <lang xml:lang="en">Adjusts direction of the motor.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = The motor goes clockwise when latched</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = The motor goes counterclockwise when
latched</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="2" type="constant" size="1" default="03">
            <name>
                <lang xml:lang="en">Speed (Torque)</lang>
              </name>
                  <description>
                <lang xml:lang="en">Adjusts speed</lang>
              </description>
      
                  <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Slowest</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">2 = Slow</lang>
                  </description>
              </value>
            <value from="03" to="03">
                <description>
                    <lang xml:lang="en">3 = Normal</lang>
                  </description>
              </value>
            <value from="04" to="04">
                <description>
                    <lang xml:lang="en">4 = Fast</lang>
                  </description>
              </value>
            <value from="05" to="05">
                <description>
                    <lang xml:lang="en">5 =
Fastest</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="3" type="constant" size="1" default="01">
            <name>
                <lang xml:lang="en">Mode</lang>
              </name>
                  <description>
                <lang xml:lang="en">Selects mode of stepper motor </lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = Stepper motor mode wave drive(power
saving)</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Stepper motor mode full
drive(Normal)</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">2 = Relay mode</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="4" type="range" size="1" default="09">
            <name>
                <lang xml:lang="en">Turn degrees</lang>
              </name>
                  <description>
                <lang xml:lang="en">Factor 10 turn degrees</lang>
              </description>
      
                  <value from="01" to="64">
                <description>
                    <lang xml:lang="en">1 = 10 degrees, 9 = 90
degrees etc.</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="5" type="range" size="1" default="00">
            <name>
                <lang xml:lang="en">Auto Latch time</lang>
              </name>
                  <description>
                <lang xml:lang="en">How many seconds from the lock has been
unlatched to it automatically should close
again.</lang>
              </description>
      
                  <value from="00" to="00" unit="sec">
                <description>
                    <lang xml:lang="en">If 0 then it is disabled.</lang>
                  </description>
              </value>
            <value from="01" to="3c" unit="sec">
                <description>
                    <lang xml:lang="en">Sets time interval till the lock should close automatically.</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="6" type="constant" size="1" default="01">
            <name>
                <lang xml:lang="en">Sound</lang>
              </name>
                  <description>
                <lang xml:lang="en">Disable or enable the beep sound from latch or
unlatch operations.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = Disable</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Enable</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="7" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Battery Type</lang>
              </name>
                  <description>
                <lang xml:lang="en">Set the type of battery that powers the device.
This affects the returned battery state.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = 3,6
Volt battery</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = 3.0 Volt battery</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">2 = Mains
power, changing from and to mains power
requires the Danalock to be excluded and
included</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="8" type="range" size="1" default="00">
            <name>
                <lang xml:lang="en">Battery alarm value</lang>
              </name>
                  <description>
                <lang xml:lang="en">When the battery level is equal to or under this
value, the device will notify user with a beep
sound after latch or unlatch.</lang>
              </description>
      
                  <value from="00" to="64">
                <description>
                    <lang xml:lang="en">Sets the battery level that is reported.</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="9" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Turn&amp;Go</lang>
              </name>
                  <description>
                <lang xml:lang="en">Used if
the lock is using an optional rotation sensor.
Latch&amp;Go on will turn automatically when
manual turn is detected.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = Turn&amp;Go off</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Turn&amp;Go on</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="10" type="range" size="1" default="00">
            <name>
                <lang xml:lang="en">Brake&amp;GoBack</lang>
              </name>
                  <description>
                <lang xml:lang="en">When
used the lock will brake for x amount of
seconds and then turn 75 degrees back. Made
for special doors without door lever.(Only
when unlatching).</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0=disabled</lang>
                  </description>
              </value>
            <value from="01" to="0f" unit="sec">
                <description>
                    <lang xml:lang="en">Define the time amount to brake and turn back.</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="11" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Async</lang>
              </name>
                  <description>
                <lang xml:lang="en">Used if the lock
is using an optional rotation sensor. When
async is enabled the lock will auto calibrate if
already unlatched and asked to Unlatch again
(used for special door locks where the key
turn is asynced from the inside nob.)</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = Async off</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Async on</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="12" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Door lock operation report type</lang>
              </name>
                  <description>
                <lang xml:lang="en">Select door lock operation report type that is sent.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = Simple, supported by all controllers that
support door locks.</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = Advanced, more
specific reports through notification
Command Class.</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">2 = No Door lock operation
reports are sent unsolicited.</lang>
                  </description>
              </value>
                </configParam>
      </configParams>
  
    
  
