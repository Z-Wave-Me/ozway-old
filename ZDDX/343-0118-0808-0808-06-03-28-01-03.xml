              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="de">Nein</lang>
                    <lang xml:lang="en">dont poll</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="de">Ja</lang>
                    <lang xml:lang="en">poll</lang>
                  </description>
              </value>
                </configParam>
      </configParams>
  
    
  
    
