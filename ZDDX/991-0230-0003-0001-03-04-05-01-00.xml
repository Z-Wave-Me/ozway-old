    <configParams>
        <configParam number="1" type="constant" size="1" default="01">
            <name>
                <lang xml:lang="en">Door Lock Mode</lang>
              </name>
                  <description>
                <lang xml:lang="en">parameter defines the door lock mode</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">Disable Away, Manual Lock</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">Disable Away, Auto Lock</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">Enable Away, Manual Lock</lang>
                  </description>
              </value>
            <value from="03" to="03">
                <description>
                    <lang xml:lang="en">Enable Away, Auto Lock</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="2" type="constant" size="2" default="0000">
            <name>
                <lang xml:lang="en">RFID Register Mode</lang>
              </name>
                  <description>
                <lang xml:lang="en">parameter controls RFID registration</lang>
              </description>
      
                  <value from="0001" to="0001">
                <description>
                    <lang xml:lang="en">Begin RFID Registering mode on the door lock</lang>
                  </description>
              </value>
            <value from="0007" to="0007">
                <description>
                    <lang xml:lang="en">RFID Database clear</lang>
                  </description>
              </value>
            <value from="0008" to="0008">
                <description>
                    <lang xml:lang="en">RFID Registering mode stop</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="3" type="constant" size="1" default="00">
