                  </description>
              </value>
            <value from="00" to="64" unit="%">
                <description>
                    <lang xml:lang="de">%</lang>
                    <lang xml:lang="en">%</lang>
                    <lang xml:lang="ru">%</lang>
                  </description>
              </value>
                </configParam>
